<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="foreign_income_type_form_view" model="ir.ui.view">
            <field name="name">ats.foreign_income_type.form</field>
            <field name="model">ats.foreign.income.type</field>
            <field name="arch" type="xml">
                <form string="Foreign Income Type">
                    <sheet>
                        <div class="oe_title">
                            <label for="value"/>
                            <h1>
                                <field name="value" placeholder="Value"/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                            <group>
                                 <field name="start_date"/>
                                 <field name="end_date"/>
                            </group>

                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="foreign_income_type_tree_view" model="ir.ui.view">
            <field name="name">ats.foreign_income_type.tree</field>
            <field name="model">ats.foreign.income.type</field>
            <field name="arch" type="xml">
                <tree string="Foreign Income Type">
                    <field name="value"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>


        <record id="foreign_income_type_act_window" model="ir.actions.act_window">
            <field name="name">ModelTitle</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ats.foreign.income.type</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="Foreign Income Type" id="foreign_income_type_menu"
                  parent="menu_ats" action="foreign_income_type_act_window"/>

    </data>
</odoo>